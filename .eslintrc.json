{
    "env": {
        "browser": true
    },
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/prefer-default-export": "off",
        "jsx-a11y/anchor-is-valid": "off"
    },
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "webpack-config/common.js"
            }
        }
    }
}